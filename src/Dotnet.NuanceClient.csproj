<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>Esteves.NuanceClient</PackageId>
    <Authors>esteveslean</Authors>
    <Description>Dotnet Nuance v4 Client</Description>
    <PackageTags>dotnet nuance v4 client</PackageTags>
    <PackageProjectUrl>https://github.com/esteveslean/dotnet-nuance-client</PackageProjectUrl>
    <RepositoryUrl>https://github.com/esteveslean/dotnet-nuance-client</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>

  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
